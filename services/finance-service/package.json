{
    "name": "fake-fintech-finance-service",
    "version": "1.0.0",
    "description": "Transaction Service for the intentionally vulnerable Fake-Fintech application (see root README). Handles payments, transfers, and transaction history. Built with Node + Express + Sequelize and deliberately shipped with loose defaults so researchers can exercise OWASP-LLM vulnerabilities.",
    "author": "Fake-Fintech Engineering",
    "license": "MIT",
    "main": "src/index.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "start": "node -r elastic-apm-node/start src/index.js",
        "dev": "nodemon --inspect=0.0.0.0:9229 -r elastic-apm-node/start src/index.js",
        "migrate": "sequelize db:migrate",
        "seed": "sequelize db:seed:all",
        "backfill:savings": "node scripts/backfill_savings_accounts.js",
        "lint": "eslint \"src/**/*.{js,jsx,mjs}\"",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand"
    },
    "dependencies": {
        "elastic-apm-node": "4.13.0",
        "@faker-js/faker": "8.4.1",
        "bcryptjs": "2.4.3",
        "body-parser": "1.20.3",
        "cors": "2.8.5",
        "dotenv": "16.6.1",
        "express": "4.21.2",
        "jsonwebtoken": "9.0.2",
        "lodash": "4.17.20",
        "mongodb": "6.19.0",
        "morgan": "1.10.1",
        "pg": "8.16.3",
        "pg-hstore": "2.3.4",
        "sequelize": "6.37.7",
        "uuid": "9.0.1",
        "winston": "3.17.0"
    },
    "devDependencies": {
        "@babel/core": "7.28.4",
        "@babel/preset-env": "7.28.3",
        "@types/jest": "29.5.14",
        "eslint": "8.57.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-import": "2.32.0",
        "jest": "29.7.0",
        "nodemon": "3.1.10",
        "sequelize-cli": "6.6.3",
        "supertest": "6.3.4"
    }
}